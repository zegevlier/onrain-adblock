{
    "description": "Adblocker for Onrain (SchoolRP's in-character social media)",
    "manifest_version": 2,
    "name": "onrain-adblock",
    "version": "1.0",
    "homepage_url": "https://github.com/zegevlier/onrain-adblock",
    "icons": {
      "48": "pen.svg"
    },
  
    "permissions": [
      "webRequest", "webRequestBlocking", "https://social.schoolrp.net/*"
    ],
  
    "background": {
      "scripts": ["background.js"]
    },
  
    "browser_specific_settings": {
      "gecko": {
        "strict_min_version": "57.0a1"
      }
    }
  }